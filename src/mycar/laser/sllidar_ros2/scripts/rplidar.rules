# set the udev rule , make the device_port be fixed by rplidar
#
# KERNEL=="ttyUSB*", ATTRS{idVendor}=="10c4", ATTRS{idProduct}=="ea60", MODE:="0777", SYMLINK+="rplidar"原文
SUBSYSTEM=="tty", KERNEL=="ttyUSB*", KERNELS=="1-1.4*", DRIVERS=="cp210x",ENV{ID_MM_DEVICE_IGNORE}="1", SYMLINK+="rplidar", MODE="0666", GROUP="dialout"



